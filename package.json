    "@tailwindcss/typography": "^0.5.0",
    "@yawnxyz/sveltekit-notion": "^0.0.533",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.15",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.11.7",
    "dotenv": "^15.0.0",
